{

    "manifest_version": 2,
    "name": "Twitter Web Panel",
    "description": "Displays Twitter in the sidebar.",
    "version": "1.0",
    "applications": {
        "gecko": {
          "strict_min_version": "54.0a1"
        }
    },

    "sidebar_action": {
        "default_title": "Twitter",
        "default_panel": "index.html"
    },

    "permissions": ["tabs"],

    "commands": {
        "_execute_sidebar_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+U"
            }
        }
    }
}